# WalletConnect Project ID (WAJIB - dapatkan dari https://cloud.walletconnect.com/)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your-actual-project-id-here

# Base Network RPC (opsional, default: https://mainnet.base.org)
NEXT_PUBLIC_BASE_RPC_URL=https://mainnet.base.org

# App Configuration
NEXT_PUBLIC_APP_NAME=O'Watch.ID
NEXT_PUBLIC_APP_URL=https://owatch.vercel.app

# Smart Contract Deployment
PRIVATE_KEY=your_private_key_here

# Base Network RPC URLs (untuk deployment)
BASE_MAINNET_RPC=https://mainnet.base.org
BASE_SEPOLIA_RPC=https://sepolia.base.org

# Block Explorer API Keys
BASESCAN_API_KEY=your_basescan_api_key_here

# OWATCH Contract Address (akan diisi setelah deployment)
NEXT_PUBLIC_OWATCH_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000
